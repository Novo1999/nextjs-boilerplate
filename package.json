{
  "name": "nextjs-boilerplate",
  "version": "0.1.0",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "production": "next start -p 3000",
    "lint": "next lint",
    "lint:fix": "next lint --fix --ignore-path .gitignore .",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "security-check": "yarn audit --groups dependencies",
    "format": "prettier --write \"./**/*.{ts,tsx,js,jsx,css}\"",
    "format-check": "prettier --check \"./**/*.{ts,tsx,js,jsx,css}\"",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate"
  },
  "dependencies": {
    "@formatjs/intl-localematcher": "^0.6.1",
    "negotiator": "^1.0.0",
    "next": "15.3.0",
    "next-themes": "^0.4.6",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.3",
    "@types/negotiator": "^0.6.3",
    "@types/node": "^22.14.1",
    "@types/react": "^19.1.1",
    "@types/react-dom": "^19.1.2",
    "eslint": "^9.24.0",
    "eslint-config-next": "15.3.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "tailwindcss": "^4.1.3",
    "typescript": "^5.8.3"
  },
  "packageManager": "yarn@4.9.1"
}
